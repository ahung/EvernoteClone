<h3> Rename Notebook </h3>

<form id="edit-notebook" data-id="<%= notebook.id %>">
  <div class="form-group">
    <label for="notebook_name">Notebook Name</label>
    <input type="text" class="form-control" name="notebook[name]" 
      id="notebook_name" value="<%= notebook.escape('name') %>">
  </div>
  
  <input type="submit" class="btn btn-primary" value="Rename Notebook">
</form>